{
    "compileOnSave": true,
    "compilerOptions": {
        "alwaysStrict": true,
        "diagnostics": true,
        "inlineSourceMap": true,
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "target": "es2015"
    }
}
